<!DOCTYPE html>
<html lang="en">
<head>
    {# meta block #}
    {% block meta %}
        {% include "templates/partials/meta.njk" %}
    {% endblock %} 

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@300;400;700&display=swap" rel="stylesheet">

    <!-- Main CSS file -->
    <!-- build:css ./css/main.min.css -->
    <link rel="stylesheet" href="/css/main.css" />
    <!-- endbuild -->
</head>
<body>  
    <!--[if lt IE 10]> 
        <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a target="_blank" href="http://outdatedbrowser.com/">upgrade your browser</a> to improve your experience.</p>
    <![endif]-->

    {# preloader block #}
    {% block preloader %}
        {% include "templates/partials/preloader.njk" %}
    {% endblock %} 

    {# header block #}
    {% block header %}
        {% include "templates/partials/header.njk" %}
    {% endblock %} 

    {# main content block #}    
    <main class="site-main">
        {% block content %}{% endblock %}
    </main>    

    {# footer block #}
    {% block footer %}
        {% include "templates/partials/footer.njk" %}
    {% endblock %} 

    <!-- Vendor JS file(s) -->
    <!-- build:js ./js/vendor.min.js -->
    <script type="text/javascript" src="../node_modules/uikit/dist/js/uikit.min.js"></script>
    <script type="text/javascript" src="../node_modules/uikit/dist/js/uikit-icons.min.js"></script>
    <!-- endbuild -->

    <!-- Main JS file -->
    <!-- build:js ./js/main.min.js -->
    <script type="text/javascript" src="/js/main.js"></script>
    <!-- endbuild -->
</body>
</html>